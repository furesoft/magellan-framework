<Application 
    x:Class="TaxCalculator.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Brushes.xaml" />
                <ResourceDictionary Source="/Resources/Buttons.xaml" />
                <ResourceDictionary Source="/Resources/Frames.xaml" />
                <ResourceDictionary Source="/Resources/Layouts.xaml" />
                <ResourceDictionary Source="/Resources/Pages.xaml" />
                <ResourceDictionary Source="/Resources/Text.xaml" />
                <ResourceDictionary Source="/Resources/Windows.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
